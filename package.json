{
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "scripts": {
    "dev": "concurrently \"npm run start:db\" \"npm run start:be\" \"npm run start:fe\"",
    "start:fe": "npm --prefix web run dev",
    "start:be": "npm --prefix api run start:dev",
    "start:db": "npm --prefix db run start:dev"
  }
}
